<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.j4h.mall.mapper.system.LogMapper">
    <select id="queryLogByName" resultType="com.j4h.mall.model.system.LogVo">
        select `id`,`admin`,`ip`,`type`,`action`,`status`,`result`,`comment`,add_time,update_time,deleted
        from cskaoyan_mall_log where `admin` like #{name}
    </select>
</mapper>
